{
  "name": "QingLong",
  "version": "1.0.0",
  "slug": "qinglong",
  "description": "一个支持 Python3、Shell、JS 脚本的定时任务管理平台。",
  "arch": ["aarch64", "armv7", "amd64"],
  "startup": "before",
  "boot": "auto",
  "host_network": false,
  "ports": {
    "5700/tcp": 5700
  },
  "map": [
    "addon_configs:/ql/data"
  ],
  "options": {
    "timezone": "Asia/Shanghai"
  },
  "schema": {
    "timezone": "str"
  }
}
