{
  "name": "青龙面板",
  "version": "latest",
  "slug": "qinglong",
  "description": "一个支持 Python3、Shell、JS 脚本的定时任务管理平台。",
  "url": "https://github.com/LemonCrab666/hassio-addons/tree/master/qinglong",
  "arch": ["aarch64", "amd64", "armv7"],
  "image": "whyour/qinglong",
  "startup": "services",
  "boot": "auto",
  "ports": {
    "5700/tcp": 5700
  },
  "map": [
    "addon_config:rw"
  ],
  "options": {
    "TZ": "Asia/Shanghai"
  },
  "schema": {
    "TZ": "str"
  },
  "env": {
    "TZ": "Asia/Shanghai"
  },
  "ingress": true,
  "ingress_port": 5700,
  "ingress_stream": true,
  "panel_icon": "mdi:web",
  "webui": "http://[HOST]:[PORT:5700]",
  "panel_title": "青龙面板",
  "volumes": {
  "/ql/data": "/addon_config/qinglong"
  }
}
