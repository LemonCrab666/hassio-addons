{
  "name": "Qinglong"，
  "version": "latest"，
  "slug": "qinglong"，
  "description": "青龙面板，一个支持 Python3、Shell、JS 脚本的定时任务管理平台。"，
  "url": "https://github.com/yourusername/qinglong-addon/qinglong"，
  "arch": ["aarch64"， "amd64"， "armv7"]，
  "image": "whyour/qinglong"，
  "startup": "services"，
  "boot": "auto"，
  "ports": {
    "5700/tcp": 5700
  }，
  "map": [
    "config:rw"
  ]，
  "options": {
    "TZ": "Asia/Shanghai"
  }，
  "schema": {
    "TZ": "str"
  }，
  "env": {
    "TZ": "Asia/Shanghai"
  }，
  "volume": {
    "/ql/data": "/config/qinglong"
  }
}
