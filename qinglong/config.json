{
  "name": "Qinglong",
  "version": "1.0.0",
  "slug": "qinglong",
  "description": "一个支持 Python3、Shell、JS 脚本的定时任务管理平台。",
  "startup": "services",
  "boot": "auto",
  "url": "https://github.com/whyour/qinglong",
  "arch": ["aarch64", "amd64", "armv7", "armhf"],
  "image": "ghcr.io/whyour/qinglong:latest",
  "options": {
    "data_folder": "/config/addons_config/ql_data"
  },
  "schema": {
    "data_folder": "str"
  },
  "ports": {
    "5700": 5700
  },
  "map": [
    "config:rw",
    "addons_config:rw"
  ],
  "services": [
    {
      "name": "qinglong",
      "service": "qinglong",
      "restart": "always",
      "command": "/bin/bash -c 'docker-entrypoint.sh'"
    }
  ]
}
